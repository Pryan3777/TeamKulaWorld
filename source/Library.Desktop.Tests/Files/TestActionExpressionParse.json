{
  "object GameObject Scene": {
    "string Name": "Root",
    "integer Counter": 0,
    "float Angle": 0.0,
    "object ActionExpression Actions": [
      {
        "Expression": "<<<this.Name == Root>>>"
      },
      {
        "Expression": "<<<++ this.Counter>>>"
